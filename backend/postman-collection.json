      ]
    },
    {
      "name": "üõçÔ∏è Products",
      "item": [
        {
          "name": "Get All Products",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/products?page=1&limit=20&sortBy=created_at&sortOrder=desc",
              "host": ["{{base_url}}"],
              "path": ["products"],
              "query": [
                { "key": "page", "value": "1" },
                { "key": "limit", "value": "20" },
                { "key": "sortBy", "value": "created_at" },
                { "key": "sortOrder", "value": "desc" }
              ]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.data && jsonData.data.length > 0) {",
                  "    pm.collectionVariables.set('product_id', jsonData.data[0]._id || jsonData.data[0].id);",
                  "}",
                  "pm.test('Status code is 200', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "pm.test('Has products array', function () {",
                  "    pm.expect(jsonData.data).to.be.an('array');",
                  "});"
                ]
              }
            }
          ]
        },
        {
          "name": "Get Products by Category",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/products?category={{category_id}}&page=1&limit=20",
              "host": ["{{base_url}}"],
              "path": ["products"],
              "query": [
