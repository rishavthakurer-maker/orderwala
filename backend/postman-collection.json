{
  "info": {
    "name": "Orderwala API Collection",
    "description": "Complete API collection for Orderwala food delivery platform. Includes Authentication, Products, Orders, Cart, Payments, Delivery, Vendors, Maps, and Admin endpoints.",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    { "key": "base_url", "value": "http://localhost:3000/api" },
    { "key": "backend_url", "value": "http://localhost:5000/api" },
    { "key": "token", "value": "" },
    { "key": "user_id", "value": "" },
    { "key": "vendor_id", "value": "" },
    { "key": "product_id", "value": "" },
    { "key": "order_id", "value": "" },
    { "key": "category_id", "value": "" },
    { "key": "address_id", "value": "" }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [{ "key": "token", "value": "{{token}}", "type": "string" }]
  },
  "item": [
    {
      "name": "üîê Authentication",
      "item": [
        {
          "name": "Sign Up - Customer",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": { "raw": "{{base_url}}/auth/signup", "host": ["{{base_url}}"], "path": ["auth", "signup"] },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"phone\": \"+919876543210\",\n  \"password\": \"SecurePass123\",\n  \"role\": \"customer\"\n}"
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.token) {",
                  "    pm.collectionVariables.set('token', jsonData.token);",
                  "}",
                  "if (jsonData.user && jsonData.user.id) {",
                  "    pm.collectionVariables.set('user_id', jsonData.user.id);",
                  "}",
                  "pm.test('Status code is 201', function () {",
                  "    pm.response.to.have.status(201);",
                  "});"
                ]
              }
            }
          ]
        },
        {
          "name": "Sign Up - Vendor",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": { "raw": "{{base_url}}/auth/signup", "host": ["{{base_url}}"], "path": ["auth", "signup"] },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Pizza Palace\",\n  \"email\": \"vendor@example.com\",\n  \"phone\": \"+919876543211\",\n  \"password\": \"VendorPass123\",\n  \"role\": \"vendor\"\n}"
            }
          }
        },
        {
          "name": "Sign Up - Delivery Partner",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": { "raw": "{{base_url}}/auth/signup", "host": ["{{base_url}}"], "path": ["auth", "signup"] },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Delivery Person\",\n  \"email\": \"delivery@example.com\",\n  \"phone\": \"+919876543212\",\n  \"password\": \"DeliveryPass123\",\n  \"role\": \"delivery\"\n}"
            }
          }
        },
        {
          "name": "Login - Email/Password",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": { "raw": "{{base_url}}/auth/login", "host": ["{{base_url}}"], "path": ["auth", "login"] },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"john@example.com\",\n  \"password\": \"SecurePass123\"\n}"
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.token) {",
                  "    pm.collectionVariables.set('token', jsonData.token);",
                  "}",
                  "if (jsonData.user && jsonData.user.id) {",
                  "    pm.collectionVariables.set('user_id', jsonData.user.id);",
                  "}",
                  "pm.test('Status code is 200', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "pm.test('Has token', function () {",
                  "    pm.expect(jsonData.token).to.exist;",
                  "});"
                ]
              }
            }
          ]
        },
        {
          "name": "Send OTP",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": { "raw": "{{base_url}}/auth/send-otp", "host": ["{{base_url}}"], "path": ["auth", "send-otp"] },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"phone\": \"+919876543210\"\n}"
            }
          }
        },
        {
          "name": "Verify OTP",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "url": { "raw": "{{base_url}}/auth/verify-otp", "host": ["{{base_url}}"], "path": ["auth", "verify-otp"] },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"phone\": \"+919876543210\",\n  \"otp\": \"123456\"\n}"
            }
          }
        },
        {
          "name": "Get Current User (Me)",
          "request": {
            "method": "GET",
            "header": [{ "key": "Authorization", "value": "Bearer {{token}}" }],
            "url": { "raw": "{{base_url}}/auth/me", "host": ["{{base_url}}"], "path": ["auth", "me"] }
          }
        },
